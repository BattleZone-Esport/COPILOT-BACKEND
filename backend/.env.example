# Core
APP_NAME=Ureshii-Partner
LOG_LEVEL=info
APP_CORS_ORIGINS=*

# MongoDB
MONGODB_URI=mongodb+srv://<user>:<pass>@cluster.mongodb.net/?retryWrites=true&w=majority
# Alias supported:
MONGO_URI=
MONGODB_DB=ureshii_partner

# OpenRouter / OpenAI SDK
OPENROUTER_API_KEY=sk-or-...
OPENROUTER_SITE_URL=https://your-site.example.com
OPENROUTER_SITE_NAME=Ureshii-Partner

# Default models (can be overridden per request)
DEFAULT_CODER_MODEL=qwen/qwen3-coder:free
DEFAULT_DEBUGGER_MODEL=deepseek/deepseek-chat-v3.1:free
DEFAULT_FIXER_MODEL=nvidia/nemotron-nano-9b-v2:free

# Queue backend: redis | qstash | none
QUEUE_BACKEND=redis

# Redis
REDIS_URL=rediss://default:...@cute-kingfish-xxxx.upstash.io:6379

# QStash
QSTASH_URL=https://qstash.upstash.io
QSTASH_TOKEN=your-qstash-token
QSTASH_CURRENT_SIGNING_KEY=sig_xxx
QSTASH_NEXT_SIGNING_KEY=sig_yyy
# Public HTTPS webhook URL for job processing
QSTASH_DESTINATION_URL=https://api.yourdomain.com/v1/webhooks/qstash
# Signature verification toggle (false by default)
QSTASH_VERIFY_SIGNATURE=true

# Prompt size guard
PROMPT_MAX_CHARS=20000
