# Core
APP_NAME=Ureshii-Partner
ENV=dev
DEBUG=true
LOG_LEVEL=INFO

# MongoDB (Motor)
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB_NAME=ureshii_partner

# Redis (optional)
REDIS_URL=redis://localhost:6379/0

# Upstash QStash (optional)
QSTASH_TOKEN=
# Public HTTPS URL for your webhook endpoint that QStash will call
QSTASH_WEBHOOK_URL=https://your-domain.com/api/v1/webhooks/qstash
# Optional signing keys if you implement verification
QSTASH_CURRENT_SIGNING_KEY=
QSTASH_NEXT_SIGNING_KEY=

# OpenRouter
OPENROUTER_API_KEY=
OPENROUTER_MODEL=deepseek/deepseek-coder
# Optional: identify your app to OpenRouter
OPENROUTER_SITE_URL=https://your-domain.com
OPENROUTER_APP_NAME=Ureshii-Partner
